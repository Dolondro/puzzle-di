{
  "name": "downsider/puzzle-di",
  "type": "composer-plugin",
  "description": "Puzzle DI, auto configuration for dependency injection in modules",
  "license": "proprietary",
  "homepage": "https://github.com/downsider/puzzle-di",
  "require": {
    "php": ">=5.3.0",
    "composer-plugin-api": "^1.0"
  },
  "require-dev": {
    "composer/composer": "1.0.*@dev",
    "phpunit/phpunit": "4.*",
    "mockery/mockery": "0.9.*"
  },
  "autoload": {
    "psr-4": {
      "Downsider\\PuzzleDI\\": "src"
    }
  },
  "extra": {
    "class": "Downsider\\PuzzleDI\\Plugin\\PuzzlePlugin"
  }
}